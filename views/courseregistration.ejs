<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="css/app.css" />
    <link rel="stylesheet" href="css/studentmanager.css" />
    <link rel="stylesheet" href="css/courseregistration.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.css"
    />
    <title>Dashboard</title>
  </head>
  <body>
    <nav>
      <ul class="side-bar">
        <li>
          <a href="/"> <i class="fa fa-dashboard"></i>Dashboard</a>
        </li>
        <li>
          <a href="/student-manager">
            <i class="fa fa-user"></i>Student Manager</a
          >
        </li>
        <li>
          <a href="/coursemanager"><i class="fa fa-file"></i>Course Manager</a>
        </li>
        <li>
          <a href="/course-registration"
            ><i class="fa fa-cube"></i>Course Registration</a
          >
        </li>
      </ul>
    </nav>
    <div class="main">
      <div class="header">
        <div class="logo">
          <img
            src="img/CrescentLogo.jpeg"
            alt="Crescent Logo"
            style="width: 30px; height: 30px"
          />
          <p>
            Crescent University <br />
            Abeokuta (CUAB)
          </p>
        </div>
        <div>
          <H3>Welcome Back Admin</H3>
        </div>
        <div class="menu-line">
          <i class="fa fa-navicon"></i>
        </div>
      </div>
      <div class="table-info">
        <div class="search-bar">
          <h3>Matric No</h3>
          <input type="text" id="search_input" />
          <button id="search_btn">Fetch</button>
          <!-- <button id="search_btn">
            <a href="/student-manager">Fetch all</a>
          </button> -->
        </div>
        <div class="registration-container">
          <div id="student_info"></div>
          <div id="dept_lvl"></div>
          <div id="course_list"></div>
        </div>
      </div>
    </div>

    <script src="js/registration.js"></script>
  </body>
</html>
